{
	"name": "Ansible Playground",
	"dockerComposeFile": "../docker-compose.yml",
	"service": "ansible",
	"workspaceFolder": "/ansible",
	"extensions": [
		"vscoss.vscode-ansible"
	],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"shutdownAction": "stopCompose"
}